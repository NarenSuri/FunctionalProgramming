<h1> Edit Page </h1>

<%= form_for @changesetForEdit, topic_path(@conn, :update, @fetchRecord), fn f -> %>

  <div class="form-group">
    <%= text_input f, :title, placeholder: "Title", class: "form-control" %>
    <%= error_tag f, :title %>
  </div>

  <%= submit "Save Topic", class: "btn btn-primary" %>

<% end %>